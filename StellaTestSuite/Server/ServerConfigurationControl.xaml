<UserControl x:Class="StellaTestSuite.Server.ServerConfigurationControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:StellaTestSuite.Server"
             d:DataContext="{d:DesignInstance local:ServerConfigurationViewModel}"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
        <TextBlock Text="Server settings" FontWeight="Bold"/>
        <TextBlock Text="Storyboard dir"/>
        <TextBox MaxLines="1"/>

    </StackPanel>
</UserControl>
